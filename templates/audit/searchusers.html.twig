<div id="search-result" class="col-md-12">
    {% if users is not empty %}
    <table class="table">
        <tbody>
    {% for user in users %}
        <tr>
            <td class="select-user">{{ user.username }}</td>
        </tr>
    {% endfor %}
        </tbody>
    </table>

    {% else %}
    <div class="row">
        <div class="col-md-12">
            <span>Aucune donnée ou accès déjà donné</span>
        </div>
    </div>
    {% endif %}
</div>